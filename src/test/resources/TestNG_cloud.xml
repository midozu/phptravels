<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">

<suite name="POM_net_phptravels">
	<listeners>
		<listener class-name="commons.TestNGListener"></listener>
	</listeners>

	<test name="Test on Chrome">
		<parameter name="browser" value="chrome" />
		<parameter name="url" value="http://www.phptravels.net/en" />
		<classes>
			<class name="testcases_cloud.Travels_SignUp_cloud"></class>
			<class name="testcases_cloud.Travels_LogIn_cloud"></class>
			<class name="testcases_cloud.Travels_Bookings_cloud"></class>
		</classes>
	</test>

